{
    "family": "echo-task-definition",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "echo-service",
            "image": "public.ecr.aws/j8r2p7b6/echo-server:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "8080"
                },
                {
                    "name": "ALL_PROXY",
                    "value": "socks5h://127.0.0.1:15080"
                }
            ]
        }
    ]
}
